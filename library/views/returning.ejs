<% include head %>
    <% include partials/topbar-student %>
    <% if(error) { %>
        <div class="alert alert-danger"><%= error %></div>
    <% } %>
    <div class="bookListWrap blockWrap meWrap">
        <div class="selectWrap">
            <select id="returnOrNot">
                <option value="borrow">未还图书</option>
                <option value="returning">已还图书</option>
            </select>
        </div>
        <h6 class="mb30 b">已还图书</h6>
        <% if (books.length ===0) { %>
            <div class="notFound">
                木有已经还了的书!
            </div>
        <% } else { %>
            <div class="bookList">
                <%- include partials/bookItem-borrow %>
            </div>
        <% } %>
    </div>
    <% include partials/footer %>

<% include foot %>